print("k1 =",k1,"\n")

print("fxk2:",fxk2, "\nfyk2:",fyk2)
print("k2 =",k2,"\n")

print("fxk3:",fxk3, "\nfyk3:",fyk3)
print("k3 =",k3,"\n")

print("fxk4:",fxk4, "\nfyk4:",fyk4)
print("k4 =",k4)

y4 = y3+ ((h/6)*(k1+(2*k2)+(2*k3)+k4))

print("\n\ny4 =",y4,"\n\n\n")

#/////////////////////////////////////////////////

x4 = x3+h


fxk1 = x4
fyk1 = y4
k1= -0.523* (fyk1-20)

fxk2 = (x4+h/2)
fyk2 = y4+((h*k1)/2)
